{
  "manifest_version": 3,
  "name": "CoffeeFilter",
  "description": "Filters out Starbucks, Dunkin', and McDonald's from Google Maps.",
  "version": "1.1.0",

  "action": {
    "default_popup": "pages/popup.html",

    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://www.google.com/maps/*"],
      "js": ["scripts/content.js"]
    }
  ],
  "permissions": ["tabs"]
}
